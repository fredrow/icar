<% if event.signup.present? %>
  <%= image_tag('round-table/signup.svg', title: t('.signup')) %>
<% end %>

<% if event.food? %>
  <%= image_tag('round-table/food.svg', title: t('.food')) %>
<% end %>

<% if event.drink? %>
  <%= image_tag('round-table/alcohol.svg', title: t('.drink')) %>
<% end %>

<% if event.cash? %>
  <%= image_tag('round-table/cash.svg', title: t('.cash')) %>
<% end %>
